    "@newlevelup/syncpack-config": "1.0.0",
    "@types/node": "18.11.18",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "npm-run-all": "4.1.5",
    "syncpack": "8.4.11"
  },
  "keywords": [
    "code",
    "flags",
    "mods",
    "removal",
    "taotie",
    "typescript"
  ],
  "commitlint": {
    "extends": [
      "@newlevelup"
    ]
  },
  "lint-staged": {
    "package.json": [
