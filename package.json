{
  "name": "@taotiebot/codemods",
  "version": "0.0.1",
  "private": false,
  "license": "Apache 2.0",
  "description": "A module containing typescript code modifiers for feature flag removal",
  "author": "",
  "homepage": "https://github.com/taotiebot/codemods#readme",
  "repository": "taotiebot/codemods.git",
  "engines": {
    "node": "19.2.0",
    "pnpm": ">=7.18.1",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm"
  },
  "packageManager": "pnpm@7.18.1",
  "bugs": "https://github.com/taotiebot/codemods/issues",
  "main": "index.js",
  "scripts": {
    "format": "npm-run-all --parallel format:*",
